{
  "name": "CalTab",
  "manifest_version": 2,
  "version": "3.0",
  
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  
  "permissions": [
            "storage"
            ],
  
  "content_security_policy": "script-src 'self'; object-src 'self'; style-src 'self'; font-src 'self'",
  
  "icons": { "16": "Icons/tab_16.png",
             "48": "Icons/tab_48.png",
             "128": "Icons/tab_128.png" 
  },

  "chrome_url_overrides" : {
    "newtab": "info.html"
  }
}
